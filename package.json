{
  "name": "zts-simpleapi",
  "version": "0.0.1",
  "description": "Simple HTTP API to Zotero ",
  "main": "lib/server.js",
  "scripts": {
    "prestart": "npm run compile",
    "start": "node lib/server.js",
    "watch": "nodemon --exitcrash -w src -e coffee,yml src/lib/server.coffee",
    "pretest": "npm run compile",
    "test": "node test/*.js",
    "prepublish": "npm run compile",
    "compile": "coffee -o . src",
    "clean": "rm -rf lib test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kba/zts-in-a-box.git"
  },
  "author": "kba",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/kba/zts-in-a-box/issues"
  },
  "homepage": "https://github.com/kba/zts-in-a-box#readme",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "async": "^2.0.1",
    "coffee-script": "^1.10.0",
    "express": "^4.14.0",
    "glob": "^7.0.5",
    "http-proxy-middleware": "^0.17.1",
    "mkdirp": "^0.5.1",
    "morgan": "^1.7.0",
    "mustache": "^2.2.1",
    "superagent": "^3.3.1",
    "traf": "1.0.0"
  },
  "devDependencies": {
    "ajv": "^4.5.0",
    "nodemon": "^1.10.0",
    "supertest": "^2.0.0",
    "tape": "^4.6.0"
  }
}
